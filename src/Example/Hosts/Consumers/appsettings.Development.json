{
  "Features": {
    "Logging": {
      "Order": 0,
      "Serilog": {
        "MinimumLevel": "Debug",
        "WriteTo": [
          {
            "Name": "Console"
          },
          {
            "Name": "Elasticsearch",
            "Args": {
              "nodeUris": "http://localhost:9200",
              "typeName": "logevent",
              "indexFormat": "meta-logs-{0:yyyy.MM.dd}"
            }
          }
        ]
      }
    }
  },

  "RabbitMqService": {
    "Hosts": [ "localhost", "localhost" ], // Два раза написано, чтобы проверить работу с двумя хостами.
    "UserName": "guest",
    "Password": "guest",
    "VirtualHost": "Meta",
    "Port": "5672"
  },

  "WeatherForecastClient": {
    "Url": "http://localhost:5102"
  }
}
